{{#each responseFromAPI}}

<div class="radom_beer">
<img src="{{this.image_url}}" alt="beer image">
<h3>Name: {{this.name}}</h3>
<h4>Description: {{this.description}}</h4>
<p>Tagline: {{this.tagline}}</p>
<p>Food Pairing{{this.food_pairing}}</p>
<p>Brewer Tips{{this.brewer_tips}}</p>
</div>
{{/each}}

